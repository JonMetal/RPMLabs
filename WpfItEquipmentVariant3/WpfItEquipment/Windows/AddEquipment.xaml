<Window x:Class="WpfItEquipment.Windows.AddEquipment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfItEquipment.Windows"
        mc:Ignorable="d"
        Title="AddEquipment" Height="250" Width="300">
    <Grid>
        <TextBox x:Name="textBoxModel" HorizontalAlignment="Left" Margin="10,11,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="comboBoxEquipmentType" HorizontalAlignment="Left" Margin="10,34,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Model" HorizontalAlignment="Left" Margin="135,7,0,0" VerticalAlignment="Top"/>
        <Label Content="Equipment Type" HorizontalAlignment="Left" Margin="135,32,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonAdd" Content="Add" HorizontalAlignment="Left" Margin="10,61,0,0" VerticalAlignment="Top" Click="ButtonAdd_Click"/>
        <Button x:Name="ButtonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="90,61,0,0" VerticalAlignment="Top" Click="ButtonCancel_Click"/>
    </Grid>
</Window>
