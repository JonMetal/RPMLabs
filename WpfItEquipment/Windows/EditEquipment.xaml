<Window x:Class="WpfItEquipment.Windows.EditEquipment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfItEquipment.Windows"
        mc:Ignorable="d"
        Title="EditEquipment" Height="150" Width="300">
    <Grid>
        <TextBox x:Name="textBoxId" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBoxModel" HorizontalAlignment="Left" Margin="10,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <ComboBox x:Name="comboBoxEquipmentType" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Id" HorizontalAlignment="Left" Margin="135,6,0,0" VerticalAlignment="Top"/>
        <Label Content="Model" HorizontalAlignment="Left" Margin="135,29,0,0" VerticalAlignment="Top"/>
        <Label Content="Equipment Type" HorizontalAlignment="Left" Margin="135,54,0,0" VerticalAlignment="Top"/>
        <Button x:Name="ButtonConfirm" Content="Confirm" HorizontalAlignment="Left" Margin="10,83,0,0" VerticalAlignment="Top" Click="ButtonAdd_Click"/>
        <Button x:Name="ButtonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="90,83,0,0" VerticalAlignment="Top" Click="ButtonCancel_Click"/>
    </Grid>
</Window>
